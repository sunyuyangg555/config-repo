zuul:
  ignored-services: "*"
  prefix:  /api
  routes:
    organization: 
      path: /v1/organizations/**
      serviceId: organizationservice
      strip-prefix: false
    licensing:
      path: /v1/licensing/**
      serviceId: licensingservice
      strip-prefix: false    
    authentication:
      path: /auth/**
      serviceId: authenticationservice
      strip-prefix: false 

  sensitiveHeaders: Cookie,Set-Cookie
  debug:
    request: true
